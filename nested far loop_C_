#include <stdio.h>

int main() {
    int a, b, c;

    printf("Enter three numbers: ");
    scanf("%d %d %d", &a, &b, &c);

    if (a > b) {                 // First level
        if (a > c)               // Nested inside 'if'
            printf("%d is the largest.\n", a);
        else
            printf("%d is the largest.\n", c);
    } 
    else {                       // First 'else'
        if (b > c)               // Nested inside 'else'
            printf("%d is the largest.\n", b);
        else
            printf("%d is the largest.\n", c);
    }

    return 0;
}
